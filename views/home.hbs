	<div class="container intro">
		<form class="cake-form" action="https://formspree.io/holiechan@hotmail.com" method="POST">
		    <div class="form-row">
			    <div class="form-group col-md-6">
			      <label for="first-name">First Name</label>
			      <input type="text" class="form-control" id="first-name" placeholder="Enter first name" name="first name" required="true">
			    </div>
			    <div class="form-group col-md-6">
			      <label for="last-name">Last Name</label>
			      <input type="text" class="form-control" id="last-name" placeholder="Enter last name" name="last name" required="true">
			    </div>
			</div>
			<div class="form-row">
			    <div class="form-group col-md-6">
				    <label for="email">Email address</label>
			    	<input type="email" class="form-control" id="email" aria-describedby="emailHelp" name="email" placeholder="Enter email" required="true">
			    </div>
			    <div class="form-group col-md-6">
			      <label for="phone">Phone</label>
			      <input type="text" class="form-control" id="phone" placeholder="Enter phone number" name="phone" required="true">
			    </div>
			</div>
		  <div class="form-group">

		  </div>
		  <div class="form-group">
		    <label for="select">How did you hear about us? Please select one.</label>
		    <select multiple class="form-control" id="select" name="How did you hear about us" required="true">
		      <option>Instagram</option>
		      <option>Facebook</option>
		      <option>Web</option>
		      <option>From a friend</option>
		      <option>Other</option>
		    </select>
		  </div>
		  <div class="form-group">
		    <label for="message">Message</label>
		    <textarea class="form-control" id="message" name = "message" rows="5" placeholder="Enter message" required="true"></textarea>
		  </div>
		  <button type="submit" class="btn btn-secondary">Submit</button>
		</form>
